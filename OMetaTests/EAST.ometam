ometa EAST {
num = ( digit + ) : ds -> {{{ @[@"num", @([[ds componentsJoinedByString:@""] integerValue])] }}} ,
fac = num : x '*' fac : y -> {{{ @[@"mul",x,y] }}}
    | num : x '/' fac : y -> {{{ @[@"div",x,y] }}}
    | num ,
exp = fac : x '+' exp : y -> {{{ @[@"add",x,y] }}}
    | fac : x '-' exp : y -> {{{ @[@"sub",x,y] }}}
    | fac
}